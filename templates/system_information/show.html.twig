    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ system_information.id }}</td>
            </tr>
            <tr>
                <th>Ram</th>
                <td>{{ system_information.ram }}</td>
            </tr>
            <tr>
                <th>Cpu</th>
